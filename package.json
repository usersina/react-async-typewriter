{
  "name": "@usersina/react-async-typewriter",
  "version": "0.1.0",
  "description": "An asynchronous react typewriter",
  "author": "usersina",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/usersina/react-async-typewriter.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^9.0.0",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^9.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/react-async-typewriter.esm.js",
  "size-limit": [
    {
      "path": "dist/react-async-typewriter.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-async-typewriter.esm.js",
      "limit": "10 KB"
    }
  ]
}
